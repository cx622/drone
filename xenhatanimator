cpy:Button("Copy Functions",function()
setclipboard(functions)
notify("Xen's Hat Scripter","Copied to Clipboard")
end)

cpy:Button("Copy Create",function()
if mode == "Create" then
setclipboard("Create('"..hate.."',Vector3.new("..px..","..py..","..pz.."),Vector3.new("..rx..","..ry..","..rz.."),'"..parte.."')")
notify("Xen's Hat Scripter","Copied to Clipboard")
end
end)

cpy:Button("Copy Edit",function()
if mode == "Edit" then
setclipboard("Edit('"..hate.."',Vector3.new("..px..","..py..","..pz.."),Vector3.new("..rx..","..ry..","..rz.."),'"..parte.."')")
notify("Xen's Hat Scripter","Copied to Clipboard")
end
end)
cpy:Button("Copy Remove Mesh",function()
setclipboard("rmesh('"..hate.."')")
notify("Xen's Hat Scripter","Copied to Clipboard")
end)

